#include <bits/stdc++.h>

using namespace std;

int main()
{
    int tc;
    cin >> tc;
    long long int n,j=0,d=0;
    long long int c[100001];
    while(tc--)
    {
        cin >> n;
        for(int i=1; i*i<=n; i++){
        if(n%i == 0){
            {c[j]=i ;j++;}
            if(n/i != i) {c[j]=n/i;j++;}
        }
    }
    }
        for(int i=0;i<j;i++)
        {
            if(c[j]%2!=0)d+=c[j];
        }
        cout << d << endl;

    return 0;
}
